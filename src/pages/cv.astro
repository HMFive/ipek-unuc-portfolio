---
import BaseLayout from "../layouts/BaseLayout.astro";
import TimeLineElement from "../components/cv/TimeLine.astro";
---

<BaseLayout title="Resume" sideBarActiveItemID="cv">
  <button
    class="btn btn-primary rounded-full mb-5"
    type="submit"
    onclick="window.open('ipek-cv-2024.pdf')">Download CV</button
  >
  <iframe
    src="https://docs.google.com/viewer?url=https://ipekunuc.com/cv/ipek-cv-2024.pdf&embedded=true"
    style="width:600px; height:800px;"
    frameborder="0"></iframe>
</BaseLayout>
